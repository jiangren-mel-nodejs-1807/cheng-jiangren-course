{
  "name": "Course",
  "plural": "Courses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "length": {
      "type": "number",
      "required": true
    },
    "start": {
      "type": "date"
    },
    "image": {
      "type": "string"
    },
    "on": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasMany",
      "model": "Student",
      "foreignKey": "courseId",
      "through": "Enrolment"
    }
  },
  "acls": [],
  "methods": {}
}
